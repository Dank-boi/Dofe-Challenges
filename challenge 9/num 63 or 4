import turtle
turtle.pencolor("purple")
for i in range(4):
 turtle.forward(100)
 turtle.right(90)
turtle.penup()
turtle. forward(200)
turtle.pendown()
turtle.pencolor("green")
for i in range(4):
 turtle.forward(100)
 turtle.right(90)
turtle.penup()
turtle. forward(200)
turtle.pendown()
turtle.pencolor("red")
for i in range(4):
 turtle.forward(100)
 turtle.right(90)
